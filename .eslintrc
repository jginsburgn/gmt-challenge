{
  "extends": "tenatek",
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "rules": {
    "no-console": "off",
    "brace-style": [
      "error",
      "stroustrup",
      {
        "allowSingleLine": false
      }
    ],
    "max-len": "off",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-ternary": "off"
  }
}